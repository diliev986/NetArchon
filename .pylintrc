[MASTER]
ignore=venv
load-plugins=
jobs=1
max-line-length=100

[MESSAGES CONTROL]
disable=
    C0114,  ; missing module docstring
    C0115,  ; missing class docstring
    C0116,  ; missing function docstring
    R0903,  ; too few public methods (simple class)
    E0401,  ; import-error (common in virtual envs)
    W0613   ; unused arguments

[REPORTS]
reports=no

[FORMAT]
indent-string='    '